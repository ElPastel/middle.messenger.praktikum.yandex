{
    "compilerOptions": {
        "outDir": "dist",
        "target": "es2016",
        "declaration": false,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "module": "commonjs",
        "strictNullChecks": true,
        "sourceMap": true
    },
    "include": [
        "./src",
        "declarations.d.ts"
    ],
    "exclude": [
        "./node_modules",
        "./.parcel-cache",
        "./dist"
    ]
}